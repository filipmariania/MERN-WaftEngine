{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "format": "prettier --write \"src/**/*.{js,css,json}\"",
    "format:check": "prettier --list-different \"src/**/*.{js,css,json}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "prestart": "npm-run-all --parallel prod:*",
    "prod:build-server": "export NODE_ENV=production && webpack --config webpack.server.prod.js",
    "prod:build-client": "export NODE_ENV=production && webpack --config webpack.client.prod.js",
    "start": "export NODE_ENV=production && export PORT=3000 && node build/bundle.js",
    "test": "jest --silent",
    "test:update": "jest --silent -u"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apisauce": "1.0.0",
    "compression": "1.7.3",
    "express": "4.16.3",
    "express-http-proxy": "1.0.6",
    "express-ip": "1.0.3",
    "geoip-lite": "1.3.2",
    "google-map-react": "1.0.6",
    "leaflet": "1.3.4",
    "leaflet.markercluster": "1.4.1",
    "offline-plugin": "5.0.5",
    "prop-types": "15.6.2",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-helmet": "5.2.0",
    "react-materialize": "2.4.5",
    "react-redux": "5.0.7",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "redux": "4.0.0",
    "redux-thunk": "2.3.0",
    "serialize-javascript": "1.4.0",
    "webpack": "4.19.1",
    "webpack-dev-server": "3.1.8",
    "webpack-merge": "4.1.0",
    "webpack-node-externals": "1.6.0",
    "webpack-pwa-manifest": "3.7.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "9.0.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "css-loader": "1.0.0",
    "eslint": "5.6.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-react": "7.11.1",
    "jest": "23.6.0",
    "nodemon": "1.18.4",
    "npm-run-all": "4.1.1",
    "prettier": "1.14.3",
    "react-test-renderer": "16.5.2",
    "style-loader": "0.23.0",
    "webpack-cli": "3.1.0"
  }
}
